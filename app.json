{
  "expo": {
    "name": "vasbazaar",
    "slug": "vasbazaar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon/ios/iTunesArtwork@2x.png",
    "scheme": "vasbazaar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vasbazaar.app",
      "icon": "./assets/app_icon/ios/iTunesArtwork@2x.png",
      "googleServicesFile": "./google-services.json",
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to display them for recharging"
      }
    },
    "android": {
      "icon": "./assets/app_icon/android/ic_launcher-web.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon/android/ic_launcher-web.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vasbazaar.app",
      "versionCode": 1,
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "minSdkVersion": 24,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/vasbazaar_logo_vertical.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-router",
      "expo-web-browser",
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics"
    ],
    "extra": {
      "eas": {
        "projectId": "7656900a-4c68-4718-aed3-c400631ef935"
      }
    },
    "owner": "shahidhusen31"
  }
}
